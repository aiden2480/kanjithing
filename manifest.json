{
    "name": "KanjiThing",
    "description": "Learn kanji stroke order from the browser",
    "version": "1.3.1",
    "manifest_version": 3,
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "media/defaulticon.png"
    }
}